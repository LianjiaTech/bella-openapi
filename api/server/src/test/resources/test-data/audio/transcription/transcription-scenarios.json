{
  "transcriptionRequests": [
    {
      "scenarioName": "基础音频转录请求",
      "description": "基本的音频文件转录请求，使用默认参数",
      "request": {
        "url": "https://example.com/audio/recording.mp3",
        "model": "whisper-large-v3",
        "user": "user123",
        "callback_url": "https://your-domain.com/callback/transcription",
        "enable_words": true
      },
      "expectedResponse": {
        "task_id": "transcription-task-123456"
      },
      "mockChannel": {
        "channelInfo": "{\"apiKey\":\"test-key\",\"deployName\":\"whisper-large-v3\"}",
        "protocol": "openai",
        "url": "https://mock-transcription-service.com/v1/audio/transcriptions"
      },
      "parameterValidations": [
        {"field": "url", "expectedValue": "https://example.com/audio/recording.mp3"},
        {"field": "model", "expectedValue": "whisper-large-v3"},
        {"field": "user", "expectedValue": "user123"},
        {"field": "callback_url", "expectedValue": "https://your-domain.com/callback/transcription"}
      ]
    },
